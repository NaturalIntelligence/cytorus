{
  "name": "cytorus",
  "version": "0.3.0",
  "description": "Gherkin syntaxed feature file test runner using Cypress. Run tests for a particular story, route, or by their position. Define your own test strategy & reports.",
  "main": "src/cytorus.js",
  "bin": {
    "cytorus": "./cli.js"
  },
  "scripts": {
    "test": "mocha UTs/*test.js"
  },
  "keywords": [
    "cucumon",
    "runner",
    "cypress",
    "preprocessor",
    "cucumber",
    "gherkin",
    "threshold",
    "e2e",
    "parallel",
    "tests"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NaturalIntelligence/cytorus"
  },
  "author": "Amit Kumar Gupta (https://amitkumargupta.work/)",
  "license": "MIT",
  "dependencies": {
    "@cypress/browserify-preprocessor": "^3.0.1",
    "axios": "^1.7.4",
    "bexp": "^1.0.3",
    "cucumber-expressions": "^8.3.0",
    "cucumon": "^1.4.2",
    "glob": "^7.2.0",
    "strnum": "^1.0.4",
    "through": "^2.3.8"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "mocha": "^8.4.0",
    "rewire": "^6.0.0"
  }
}
