{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "runtimeVersion": "14.17.0",
            "name": "Launch Current Opened File",
            "program": "${file}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "js file",
            "program": "${workspaceFolder}/temp/temp.js",
            "cwd": "${workspaceFolder}"
        },{
            "type": "node",
            "request": "launch",
            "runtimeVersion": "14.17.0",
            "name": "Mocha debug current",
            "program": "${workspaceFolder}/node_modules/mocha/bin/mocha",
            "args": [
                //"--inspect-brk",
                "${file}"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
   
}